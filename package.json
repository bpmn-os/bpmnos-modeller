{
  "name": "bpmn-js-execution-modeler",
  "version": "0.0.0",
  "description": "A resource enabled bpmn-js modeler with execution properties",
  "main": "src/app.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-s bundle:less watch:js",
    "start": "run-s bundle serve",
    "serve": "sirv dist --dev",
    "bundle": "run-s bundle:*",
    "bundle:less": "lessc src/app.less dist/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "author": {
    "name": "Asvin Goel"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bpmn-js": "^11.4.1",
    "@bpmn-io/properties-panel": "^1.4.0",
    "bpmn-js-properties-panel": "^1.17.2",
    "bpmn-js-bpmnlint": "^0.20.1",
    "bpmnlint-utils": "^1.0.2",
    "bpmn-js-token-simulation": "^0.31.0",
    "bpmn-moddle": "^8.0.0",
    "diagram-js": "^11.9.0",
    "tiny-svg": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "copy-webpack-plugin": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^2.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "patch-package": "^6.4.7",
    "stringify": "^5.2.0"
  }
}
