{
  "name": "bpmnos-modeller",
  "version": "0.0.0",
  "description": "A BPMN modeller for optimization and simulation",
  "main": "src/app.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-s bundle:less watch:js",
    "start": "run-s bundle serve",
    "serve": "sirv dist --dev",
    "bundle": "run-s bundle:*",
    "bundle:less": "lessc src/app.less dist/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "author": {
    "name": "Asvin Goel"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bpmn-js": "^16.3.1",
    "@bpmn-io/properties-panel": "^3.15.0",
    "bpmn-js-properties-panel": "^5.7.0",
    "bpmn-js-bpmnlint": "^0.21.0",
    "bpmnlint-utils": "^1.1.1",
    "bpmn-js-token-simulation": "^0.33.0",
    "bpmn-moddle": "^8.1.0",
    "diagram-js": "^13.4.0",
    "tiny-svg": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.5.1",
    "less": "^4.1.2",
    "less-loader": "^11.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^2.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "patch-package": "^6.4.7",
    "stringify": "^5.2.0"
  }
}
